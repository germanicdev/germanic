{
  "schema_id": "en.dining.restaurant.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": false
    },
    "cuisine": {
      "type": "[string]",
      "required": true
    },
    "address": {
      "type": "table",
      "required": true,
      "fields": {
        "street": {
          "type": "string",
          "required": false
        },
        "house_number": {
          "type": "string",
          "required": false
        },
        "postal_code": {
          "type": "string",
          "required": false
        },
        "city": {
          "type": "string",
          "required": false
        },
        "country": {
          "type": "string",
          "required": false
        }
      }
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": false
    },
    "website": {
      "type": "string",
      "required": false
    },
    "rating": {
      "type": "float",
      "required": false
    },
    "price_level": {
      "type": "string",
      "required": false
    },
    "seating_capacity": {
      "type": "int",
      "required": false
    },
    "outdoor_seating": {
      "type": "bool",
      "required": false,
      "default": "false"
    },
    "wheelchair_accessible": {
      "type": "bool",
      "required": false,
      "default": "false"
    },
    "opening_hours": {
      "type": "string",
      "required": false
    },
    "reservation_url": {
      "type": "string",
      "required": false
    },
    "features": {
      "type": "[string]",
      "required": false
    },
    "languages": {
      "type": "[string]",
      "required": false
    },
    "short_description": {
      "type": "string",
      "required": false
    }
  }
}
