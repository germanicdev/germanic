{
  "schema_id": "en.dining.campground.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "operator": {
      "type": "string"
    },
    "address": {
      "type": "table",
      "required": true,
      "fields": {
        "street": {
          "type": "string",
          "required": true
        },
        "house_number": {
          "type": "string"
        },
        "postal_code": {
          "type": "string",
          "required": true
        },
        "city": {
          "type": "string",
          "required": true
        },
        "country": {
          "type": "string",
          "default": "DE"
        }
      }
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "stellplaetze_gesamt": {
      "type": "int",
      "required": true
    },
    "stellplatz_laengen_m": {
      "type": "[int]"
    },
    "preis_pro_nacht": {
      "type": "float",
      "required": true
    },
    "saison": {
      "type": "string"
    },
    "equipment": {
      "type": "[string]"
    },
    "hunde_erlaubt": {
      "type": "bool"
    },
    "strom_vorhanden": {
      "type": "bool"
    },
    "wheelchair_accessible": {
      "type": "bool"
    },
    "rating": {
      "type": "float"
    },
    "short_description": {
      "type": "string"
    }
  }
}
