{
  "schema_id": "en.public.event.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "organizer": {
      "type": "string"
    },
    "veranstaltungsort": {
      "type": "table",
      "required": true,
      "fields": {
        "name": {
          "type": "string",
          "required": true
        },
        "street": {
          "type": "string"
        },
        "postal_code": {
          "type": "string",
          "required": true
        },
        "city": {
          "type": "string",
          "required": true
        },
        "country": {
          "type": "string",
          "default": "DE"
        }
      }
    },
    "kontakt": {
      "type": "table",
      "fields": {
        "phone": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "website": {
          "type": "string"
        }
      }
    },
    "datum_beginn": {
      "type": "string",
      "required": true
    },
    "datum_ende": {
      "type": "string"
    },
    "eintritt_frei": {
      "type": "bool",
      "required": true
    },
    "kategorie": {
      "type": "string"
    },
    "programm": {
      "type": "[string]"
    },
    "erwartete_besucher": {
      "type": "int"
    },
    "wheelchair_accessible": {
      "type": "bool"
    },
    "short_description": {
      "type": "string"
    }
  }
}
