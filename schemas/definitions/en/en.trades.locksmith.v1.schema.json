{
  "schema_id": "en.trades.locksmith.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "owner": {
      "type": "string"
    },
    "address": {
      "type": "table",
      "required": true,
      "fields": {
        "street": {
          "type": "string",
          "required": true
        },
        "house_number": {
          "type": "string"
        },
        "postal_code": {
          "type": "string",
          "required": true
        },
        "city": {
          "type": "string",
          "required": true
        },
        "country": {
          "type": "string",
          "default": "DE"
        }
      }
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "preis_tueroffnung_ab": {
      "type": "float",
      "required": true
    },
    "anfahrtspauschale": {
      "type": "float",
      "required": true
    },
    "nacht_zuschlag_prozent": {
      "type": "float"
    },
    "service_area": {
      "type": "[string]",
      "required": true
    },
    "rund_um_die_uhr": {
      "type": "bool"
    },
    "handwerkskammer": {
      "type": "string"
    },
    "master_certified": {
      "type": "bool"
    },
    "short_description": {
      "type": "string"
    }
  }
}
