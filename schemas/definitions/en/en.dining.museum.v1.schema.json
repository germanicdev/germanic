{
  "schema_id": "en.dining.museum.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "operator": {
      "type": "string"
    },
    "address": {
      "type": "table",
      "required": true,
      "fields": {
        "street": {
          "type": "string",
          "required": true
        },
        "house_number": {
          "type": "string"
        },
        "postal_code": {
          "type": "string",
          "required": true
        },
        "city": {
          "type": "string",
          "required": true
        },
        "country": {
          "type": "string",
          "default": "DE"
        }
      }
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "admission": {
      "type": "table",
      "required": true,
      "fields": {
        "adults": {
          "type": "float",
          "required": true
        },
        "ermaessigt": {
          "type": "float"
        },
        "kinder_frei_bis": {
          "type": "int"
        }
      }
    },
    "sammlungen": {
      "type": "[string]"
    },
    "opening_hours": {
      "type": "string",
      "required": true
    },
    "tours": {
      "type": "bool"
    },
    "wheelchair_accessible": {
      "type": "bool"
    },
    "audioguide": {
      "type": "bool"
    },
    "languages": {
      "type": "[string]"
    },
    "rating": {
      "type": "float"
    },
    "short_description": {
      "type": "string"
    }
  }
}
