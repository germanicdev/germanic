{
  "schema_id": "de.handwerk.fotograf.v1",
  "version": 1,
  "fields": {
    "name": {
      "type": "string",
      "required": true
    },
    "fotografin": {
      "type": "string"
    },
    "adresse": {
      "type": "table",
      "required": true,
      "fields": {
        "strasse": {
          "type": "string",
          "required": true
        },
        "hausnummer": {
          "type": "string"
        },
        "plz": {
          "type": "string",
          "required": true
        },
        "ort": {
          "type": "string",
          "required": true
        },
        "land": {
          "type": "string",
          "default": "DE"
        }
      }
    },
    "telefon": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "leistungen": {
      "type": "[string]",
      "required": true
    },
    "preise": {
      "type": "table",
      "fields": {
        "portrait_ab": {
          "type": "float"
        },
        "hochzeit_ab": {
          "type": "float"
        },
        "business_ab": {
          "type": "float"
        }
      }
    },
    "sprachen": {
      "type": "[string]"
    },
    "mobil": {
      "type": "bool"
    },
    "studio_vorhanden": {
      "type": "bool"
    },
    "online_galerie": {
      "type": "bool"
    },
    "kurzbeschreibung": {
      "type": "string"
    }
  }
}