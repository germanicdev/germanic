{
  "schema_id": "de.gesundheit.apotheke.v1",
  "version": 1,
  "fields": {
    "name": { "type": "string", "required": true },
    "inhaberin": { "type": "string", "required": true },
    "adresse": {
      "type": "table",
      "required": true,
      "fields": {
        "strasse": { "type": "string", "required": true },
        "hausnummer": { "type": "string" },
        "plz": { "type": "string", "required": true },
        "ort": { "type": "string", "required": true },
        "land": { "type": "string", "default": "DE" }
      }
    },
    "telefon": { "type": "string", "required": true },
    "fax": { "type": "string" },
    "email": { "type": "string" },
    "website": { "type": "string" },
    "notdienst": { "type": "bool", "required": true },
    "lieferdienst": { "type": "bool" },
    "oeffnungszeiten": { "type": "string", "required": true },
    "schwerpunkte": { "type": "[string]" },
    "sprachen": { "type": "[string]" },
    "barrierefreiheit": { "type": "bool" },
    "parkplaetze": { "type": "int" },
    "kurzbeschreibung": { "type": "string" }
  }
}
