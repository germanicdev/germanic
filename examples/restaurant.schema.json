{
  "schema_id": "de.dining.restaurant.v1",
  "version": 1,
  "fields": {
    "name": { "type": "string", "required": true },
    "cuisine": { "type": "string" },
    "rating": { "type": "float" },
    "tags": { "type": "[string]" },
    "address": {
      "type": "table",
      "required": true,
      "fields": {
        "street": { "type": "string", "required": true },
        "city": { "type": "string", "required": true },
        "zip": { "type": "string" },
        "country": { "type": "string", "default": "DE" }
      }
    }
  }
}
